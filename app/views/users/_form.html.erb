<%= form_for(@user) do |f| %> 
<%= render 'shared/error_messages' %>
<div class="field">
	<%= f.label :firstName %><br />
	<%= f.text_field :firstName %> 
</div>
<div class="field">
	<%= f.label :lastName %><br />
	<%= f.text_field :lastName %> 
</div>
<div class="field">
	<%= f.label :email %><br /> 
	<%= f.text_field :email %>
</div>
<div class="field">
	<%= f.label :password %><br />
	<%= f.password_field :password %> 
	</div>
<div class="field">
	<%= f.label :password_confirmation, "Confirmation" %><br /> 
	<%= f.password_field :password_confirmation %>
</div>
<div class="actions">
	<%= f.submit "Sign up" %> 
</div>
<% end %>
