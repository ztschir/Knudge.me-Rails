<div id="full_form">
<div id="new_bank">
<%= form_for @user_banks, :remote => true do |f| %>
<div id="error-messages">

</div>

  <p>
    <%= f.label :content_tokens, "Add A Bank" %><br />
    <%= f.text_field :content_tokens %>
  </p>
  <p>
	<%= submit_tag "Add Bank", :id=> "addBank"%>


  </p>

<% end %>
</div>
  <div id="credentials">
    <%= render :partial => "credentials", :locals => { :credentials => nil } %>
  </div>
  <div id="banks">
    <%= render :partial => "banks", :locals => { :banks => @banks }  %>
  </div>
</div>